{
  "name": "jamieburnip/php-skeleton-framework",
  "type": "project",
  "authors": [
    {
      "name": "Jamie Burnip",
      "email": "jamieburnip@gmail.com"
    }
  ],
  "config": {
    "vendor-dir": "src/vendor"
  },
  "autoload": {
    "psr-4": {
      "App\\": [
        "src/app/",
        "tests/unit/"
