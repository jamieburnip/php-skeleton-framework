    "symfony/var-dumper": "^4.1",
    "symfony/console": "^4.1",
    "illuminate/support": "^5.6"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^7.3"
  }
}
